<LinearLayout 
             android:layout_width="match_parent" 
             android:layout_height="match_parent" 
             android:orientation="vertical"> 
  
             <TextView 
                 android:layout_width="match_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginBottom="5dp" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="@dimen/input_margin_hor" 
                 android:hint="Coupon Number" 
                 android:textSize="@dimen/hint_hin_size" /> 
  
             <EditText 
                 android:id="@+id/couponNumber" 
                 android:layout_width="match_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="5dp" 
                 android:background="@color/white" 
                 android:inputType="textCapCharacters" 
                 android:padding="@dimen/input_padding" 
                 android:textSize="@dimen/hint_hin_size" /> 
  
             <View 
                 android:layout_width="match_parent" 
                 android:layout_height="3dp" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:background="@color/colorPrimary" />                
</LinearLayout>
