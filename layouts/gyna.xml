<LinearLayout 
             android:layout_width="match_parent" 
             android:layout_height="match_parent" 
             android:orientation="vertical"> 
 						<TextView 
                 android:layout_width="match_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginBottom="5dp" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="@dimen/input_margin_hor" 
                 android:hint="Referred to Hospital/Maternity Home / further consultation" 
                 android:textSize="@dimen/hint_hin_size" /> 
  
             <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android" 
                 android:layout_width="fill_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="5dp" 
                 android:background="@color/white" 
                 android:orientation="vertical"> 
  
                 <RadioButton 
                     android:id="@+id/referredYes" 
