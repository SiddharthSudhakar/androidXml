<LinearLayout 
             android:layout_width="match_parent" 
             android:layout_height="match_parent" 
             android:orientation="vertical"> 
 						<TextView 
                 android:layout_width="match_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginBottom="5dp" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="@dimen/input_margin_hor" 
                 android:hint="How was the overall Camp?" 
                 android:textSize="@dimen/hint_hin_size" /> 
  
             <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android" 
                 android:layout_width="fill_parent" 
                 android:layout_height="wrap_content" 
                 android:layout_marginLeft="@dimen/input_margin_hor" 
                 android:layout_marginRight="@dimen/input_margin_hor" 
                 android:layout_marginTop="5dp" 
                 android:background="@color/white" 
                 android:orientation="vertical"> 
  
                 <RadioButton 
                     android:id="@+id/excellent" 
                     android:layout_width="match_parent" 
                     android:layout_height="wrap_content" 
                     android:onClick="onRadioButtonClicked" 
                     android:padding="15dp" 
                     android:text="Excellent" 
                     android:textSize="@dimen/hint_hin_size" /> 
  
                 <View 
                     android:layout_width="match_parent" 
                     android:layout_height="1dp" 
                     android:background="@android:color/darker_gray" /> 
  
                 <RadioButton 
                     android:id="@+id/good" 
                     android:layout_width="match_parent" 
                     android:layout_height="wrap_content" 
                     android:onClick="onRadioButtonClicked" 
                     android:padding="15dp" 
                     android:text="Good" 
                     android:textSize="@dimen/hint_hin_size" /> 
  
                 <View 
                     android:layout_width="match_parent" 
                     android:layout_height="1dp" 
                     android:background="@android:color/darker_gray" /> 
  
                 <RadioButton 
                     android:id="@+id/satisfactory" 
                     android:layout_width="match_parent" 
                     android:layout_height="wrap_content" 
                     android:onClick="onRadioButtonClicked" 
                     android:padding="15dp" 
                     android:text="Satisfactory" 
                     android:textSize="@dimen/hint_hin_size" /> 
  
                 <View 
                     android:layout_width="match_parent" 
                     android:layout_height="1dp" 
                     android:background="@android:color/darker_gray" /> 
  
                 <RadioButton 
                     android:id="@+id/nonSatis" 
                     android:layout_width="match_parent" 
                     android:layout_height="wrap_content" 
                     android:onClick="onRadioButtonClicked" 
                     android:padding="15dp" 
                     android:text="Non Satisfactory" 
                     android:textSize="@dimen/hint_hin_size" /> 
  
                 <View 
                     android:layout_width="match_parent" 
                     android:layout_height="1dp" 
                     android:background="@android:color/darker_gray" /> 
  </RadioGroup> 
 			            
</LinearLayout>
